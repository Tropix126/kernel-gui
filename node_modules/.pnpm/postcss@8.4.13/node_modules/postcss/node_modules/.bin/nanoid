#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Tropical/Documents/GitHub/kernel-gui/node_modules/.pnpm/nanoid@3.3.4/node_modules/nanoid/bin/node_modules:/mnt/c/Users/Tropical/Documents/GitHub/kernel-gui/node_modules/.pnpm/nanoid@3.3.4/node_modules/nanoid/node_modules:/mnt/c/Users/Tropical/Documents/GitHub/kernel-gui/node_modules/.pnpm/nanoid@3.3.4/node_modules:/mnt/c/Users/Tropical/Documents/GitHub/kernel-gui/node_modules/.pnpm/node_modules:/mnt/c/Users/Tropical/Documents/GitHub/kernel-gui/node_modules:/mnt/c/Users/Tropical/Documents/GitHub/node_modules:/mnt/c/Users/Tropical/Documents/node_modules:/mnt/c/Users/Tropical/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Tropical/Documents/GitHub/kernel-gui/node_modules/.pnpm/postcss@8.4.13/node_modules/postcss/node_modules:/mnt/c/Users/Tropical/Documents/GitHub/kernel-gui/node_modules/.pnpm/postcss@8.4.13/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/Tropical/Documents/GitHub/kernel-gui/node_modules/.pnpm/nanoid@3.3.4/node_modules/nanoid/bin/node_modules:/mnt/c/Users/Tropical/Documents/GitHub/kernel-gui/node_modules/.pnpm/nanoid@3.3.4/node_modules/nanoid/node_modules:/mnt/c/Users/Tropical/Documents/GitHub/kernel-gui/node_modules/.pnpm/nanoid@3.3.4/node_modules:/mnt/c/Users/Tropical/Documents/GitHub/kernel-gui/node_modules/.pnpm/node_modules:/mnt/c/Users/Tropical/Documents/GitHub/kernel-gui/node_modules:/mnt/c/Users/Tropical/Documents/GitHub/node_modules:/mnt/c/Users/Tropical/Documents/node_modules:/mnt/c/Users/Tropical/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Tropical/Documents/GitHub/kernel-gui/node_modules/.pnpm/postcss@8.4.13/node_modules/postcss/node_modules:/mnt/c/Users/Tropical/Documents/GitHub/kernel-gui/node_modules/.pnpm/postcss@8.4.13/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../nanoid@3.3.4/node_modules/nanoid/bin/nanoid.cjs" "$@"
else
  exec node  "$basedir/../../../../../nanoid@3.3.4/node_modules/nanoid/bin/nanoid.cjs" "$@"
fi
